%new-style
%require-types
%strict-args
%enable-all-warnings

class TelcoGetPartyId inherits Demo {
    primary() {
        Demo::primary();
        setPartyId();
    }

    string validation() {
        setPartyId();
        return Demo::validation();
    }

    private setPartyId() {
        string party_id = get_random_string();
        logInfo("got party_id %y for customer", party_id);
        setOrderKey("customer_id", party_id);
    }
}
