%new-style
%require-types
%strict-args
%enable-all-warnings

class TelcoGetAccountPartyId inherits Demo {
    primary() {
        Demo::primary();
        setAccountPartyId();
    }

    string validation() {
        setAccountPartyId();
        return Demo::validation();
    }

    private setAccountPartyId() {
        string party_id = get_random_string();
        logInfo("got party_id %y for account", party_id);
        setOrderKey("account_id", party_id);
    }
}
