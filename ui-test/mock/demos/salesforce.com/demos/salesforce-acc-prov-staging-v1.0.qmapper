# -*- mode: qore; indent-tabs-mode: nil -*-

# name: salesforce-acc-prov-staging
# version: 1.0
# desc: maps Salesforce.com account data to staging DB
# type: InboundTableMapper
# author: Qore Technologies, s.r.o.
# parse-options: PO_NEW_STYLE
# define-group: DEMO: demonstration code
# groups: DEMO

OPTION: datasource: "omquser"
OPTION: table:      "salesforce_accounts"
OPTION: input: (
    "details": {},
    "uuid": {},
    "wfiid": {},
)

FIELD: id: ("name": "details.Id")
FIELD: name: ("name": "details.Name")
FIELD: descr: ("name": "details.Description")
FIELD: acc_no: ("name": "details.AccountNumber")
FIELD: bill_city: ("name": "details.BillingCity")
FIELD: bill_country: ("name": "details.BillingCountry")
FIELD: bill_pcode: ("name": "details.BillingPostalCode")
FIELD: bill_state: ("name": "details.BillingState")
FIELD: bill_street: ("name": "details.BillingStreet")
FIELD: uuid: ("name": "uuid")
FIELD: qorus_wfiid: ("name": "wfiid")

# END
